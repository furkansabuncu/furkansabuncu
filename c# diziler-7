int[] sayilar = new int[5];

            for (int i = 0; i < 5; i += 1)
            {
                Console.WriteLine("sayÄ± giriniz");
                sayilar[i] = Convert.ToInt32(Console.ReadLine());
            }
            Array.Sort(sayilar);
