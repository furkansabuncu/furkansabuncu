class Program
    {
        static void Main(string[] args)
        {
            int x1, x2;
            x1 = 30;
            x2 = 20;
            Console.WriteLine(yazdır(ref x1, x2));
            Console.WriteLine(x1);
            Console.Read();

            
            
        }


        static int yazdır(ref int x1,int x2)
        {
            x1 = 10;
            return x1 + x2;
        }
    }
