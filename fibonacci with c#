int a,b,c;
            a = 1;
            b = 1;
           


            Console.WriteLine(a);
            Console.WriteLine(b);

            for (int i = 1; i <= 8; i += 1)
            {
                c = a + b;
                 a = b;
                 b = c;

                Console.WriteLine(c);

            }


            Console.Read();
